      "type": "dart",
      "flutterMode": "profile",
      "args": [
          "--flavor",
          "dev"
        ]
    },
    {
      "name": "Flutter_Debug Dev",
      "program": "lib/main_dev.dart",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
          "--flavor",
          "dev"
        ]
    },
    {
      "name": "Flutter_Release Dev",
      "program": "lib/main_dev.dart",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
          "--flavor",
          "dev"
        ]
    },
    {
      "name": "Flutter_Release Prod",
      "program": "lib/main_prod.dart",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
          "--flavor",
          "prod"
        ]
